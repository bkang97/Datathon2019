git config --global user.name "username"

git config --global user.email "email@mail.com"

git init

git remote add origin https://github.com/RubensZimbres/TestePush

git pull

git status

git add Hello1.py

git commit -m "initial commit"

git push -u origin master

# alter Hello1.py

git diff origin/master

--- a/Hello1.py
+++ b/Hello1.py
@@ -1 +1 @@
-print("Hello World 22")
+print("Hello World 44")

git commit -m "added while"

git push -u origin master

git log Hello1.py


### RESTORE PREVIOUS VERSION

git rebase -i HEAD~1
CTRL+X Y

git push origin +0abc3e7:master


## COMMIT

git init
git add Hello1.py
git commit -m "added x"
git push -u origin master

## DIFF

git diff 28485a4..5d908ce
